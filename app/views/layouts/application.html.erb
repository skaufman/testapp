<!DOCTYPE html>
<html>
<head>
  <title>Testapp</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="header">
    <%- if !user_signed_in? -%>
      <%= link_to("Sign up",new_user_registration_path) %>
    <%- else -%>
      Signed in as <%= current_user.email %>
    <%- end -%>
  </div>
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
<%= yield %>

</body>
</html>
